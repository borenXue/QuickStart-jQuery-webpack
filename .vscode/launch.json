  "version": "0.2.0",
  "configurations": [
{
  "type": "node",
  "request": "launch",
  "name": "build",
  "runtimeExecutable": "npm",
  "windows": {
    "runtimeExecutable": "npm.cmd"
  },
  "runtimeArgs": [
    "run-script",
    "debug"
  ],
  "port": 5858
},
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceRoot}/src/pages/entry/index.html"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Node ",
      "program": "${workspaceRoot}/build/build.js"
    },
    {
      "type": "node",
      "request": "attach",
      "name": "node-attach",
      "address": "localhost",
      "port": 5858
    }
  ]
}